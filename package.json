{
  "name": "multi-model-commit-analyzer",
  "version": "1.0.0",
  "description": "AI-powered git commit analyzer using multiple language models",
  "main": "src/server.js",
  "type": "module",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "analyze": "node src/analyzers/commitAnalyzer.js",
    "analyze:all": "node scripts/analysis/analyzeAllCommits.js",
    "analyze:missing": "node scripts/analysis/analyzeMissingCommits.js",
    "analyze:last": "node scripts/analysis/extractLastCommit.js",
    "report:daily": "node scripts/reports/generateDailyReport.js",
    "admin:reset-password": "node scripts/admin/reset-admin-password.js",
    "admin:view-db": "node scripts/admin/view-database.js"
  },
  "keywords": [
    "git",
    "commit",
    "analysis",
    "ai",
    "gpt",
    "claude",
    "gemini",
    "grok"
  ],
  "author": "Marlone Espinosa",
  "license": "MIT",
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.1",
    "@google/generative-ai": "^0.11.0",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.10.0",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-session": "^1.18.1",
    "jsonwebtoken": "^9.0.2",
    "node-fetch": "^3.3.2",
    "openai": "^4.47.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
